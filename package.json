  "bugs": {
    "url": "https://github.com/jarenella/philanthro-me/issues"
  },
  "homepage": "https://github.com/jarenella/philanthro-me#readme",
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "split-type": "^0.3.3"
  }
}
